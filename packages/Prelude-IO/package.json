{
  "name": "prelude-io",
  "version": "0.0.1",
  "description": "Type-save runtime IO library based on PreludeTS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "docs:update": "typedoc --validation"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Annoiied/PreludeIO.git"
  },
  "keywords": [
    "functional-programming",
    "io",
    "preludets"
  ],
  "author": "No√Ø van Mondfrans (they/them) <noivanmondfrans@gmail.com>",
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Annoiied/PreludeIO/issues"
  },
  "homepage": "https://github.com/Annoiied/PreludeIO#readme",
  "devDependencies": {
    "@tsconfig/node18": "1.0.1",
    "@types/jest": "^29.2.6",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.23.24",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "prelude-ts": "~1.0.6"
  },
  "prettier": {
    "overrides": [
      {
        "files": "./doc_includees",
        "options": {
          "semi": true
        }
      }
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  }
}